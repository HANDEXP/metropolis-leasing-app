<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:top="-10dp">
        <shape
            xmlns:android="http://schemas.android.com/apk/res/android"
            android:shape="ring"
            android:innerRadius="10dp"
            android:thickness="0dp"
            android:useLevel="false">

            <solid android:color="#00000000" />
            <stroke android:color="@color/white" android:width="1dp"></stroke>
        </shape>
    </item>
    <item
        android:top="5dp" android:bottom="10dp" android:left="12dp" android:right="20dp">
        <rotate android:fromDegrees="40" android:toDegrees="40" android:pivotX="50%" android:pivotY="50%">
            <shape android:shape="rectangle">
                <solid android:color="@color/white"/>
                <size android:height="1dp" android:width="6dp"/>
            </shape>
        </rotate>
    </item>
    <item
        android:top="7dp" android:bottom="8dp" android:left="17dp" android:right="10dp">
        <rotate android:fromDegrees="320" android:toDegrees="320" android:pivotX="0%" android:pivotY="50%">
            <shape android:shape="rectangle">
                <solid android:color="@color/white"/>
                <size android:height="1dp" android:width="6dp"/>
            </shape>
        </rotate>
    </item>
</layer-list>
