<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:top="-10dp" android:left="-10dp">
        <shape
            xmlns:android="http://schemas.android.com/apk/res/android"
            android:shape="ring"
            android:innerRadius="10dp"
            android:thickness="0dp"
            android:useLevel="false">

            <solid android:color="#00ababf2" />
            <stroke android:color="@color/white" android:width="1dp"></stroke>
        </shape>
    </item>
    <item
        android:top="25dp" android:bottom="4dp" android:left="50dp" android:right="30dp">
        <rotate android:fromDegrees="45" android:toDegrees="45" android:pivotX="50%" android:pivotY="50%">
            <shape android:shape="rectangle">
                <solid android:color="@color/white"/>
                <size android:height="1dp" android:width="20dp"/>
            </shape>
        </rotate>
    </item>
</layer-list>
