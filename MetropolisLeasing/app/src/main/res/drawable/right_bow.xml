<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <rotate android:fromDegrees="45" android:toDegrees="45" android:pivotX="100%" android:pivotY="20%">
            <shape android:shape="rectangle">
                <solid android:color="#cdcdcd"/>
                <size android:height="2dp" android:width="20dp"/>
            </shape>
        </rotate>
    </item>

    <item>
        <rotate android:fromDegrees="315" android:toDegrees="315" android:pivotX="100%" android:pivotY="100%">
            <shape android:shape="rectangle">
                <solid android:color="#cdcdcd"/>

                <size android:height="2dp" android:width="20dp"/>
            </shape>
        </rotate>
    </item>
</layer-list>